-- Coordenadas da NPC
local npcPosition = Vector3.new(-513, 387, -150)

-- Função para teleporte e interação
local function teleportAndInteract()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    
    -- Teleporta o jogador para a posição da NPC
    character:SetPrimaryPartCFrame(CFrame.new(npcPosition))
    
    -- Aguarda o teleporte completar
    wait(1)  -- Aguarde um segundo para garantir que o teleporte foi concluído
    
    -- Pressiona a tecla "E" por 2,5 segundos para interagir com o NPC
    local virtualInputManager = game:GetService("VirtualInputManager")
    
    -- Pressiona a tecla "E"
    virtualInputManager:SendKeyEvent(true, "E", false, game)
    wait(2.5)  -- Aguarda 2,5 segundos mantendo a tecla pressionada
    
    -- Solta a tecla "E"
    virtualInputManager:SendKeyEvent(false, "E", false, game)
end

-- Executa a função
teleportAndInteract()
